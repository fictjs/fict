{
  "name": "@fictjs/router",
  "version": "0.3.0",
  "description": "Reactive router for Fict applications",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "fict",
    "router",
    "routing",
    "spa",
    "reactive"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fictjs/fict.git",
    "directory": "packages/router"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@fictjs/runtime": "workspace:*"
  },
  "devDependencies": {
    "@fictjs/vite-plugin": "workspace:*",
    "fict": "workspace:*",
    "jsdom": "^27.4.0",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "fict": ">=0.3.0"
  },
  "peerDependenciesMeta": {
    "fict": {
      "optional": true
    }
  },
  "author": "unadlib",
  "license": "MIT"
}
